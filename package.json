{
  "name": "mobile-motmaina-app-poc-1",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "start": "expo start",
    "start:customer": "APP_VARIANT=customer APP_ENV=staging expo start --clear",
    "start:consultant": "APP_VARIANT=consultant APP_ENV=staging expo start --clear",
    "ios": "expo start --ios",
    "ios:customer": "APP_VARIANT=customer APP_ENV=staging expo start --ios --clear",
    "ios:consultant": "APP_VARIANT=consultant APP_ENV=staging expo start --ios --clear",
    "android": "expo start --android",
    "android:customer": "APP_VARIANT=customer APP_ENV=staging expo start --android --clear",
    "android:consultant": "APP_VARIANT=consultant APP_ENV=staging expo start --android --clear",
    "web": "expo start --web",
    "build:ios:customer:staging": "eas build --profile customer-staging --platform ios --local",
    "build:ios:customer:prod": "eas build --profile customer-production --platform ios --local",
    "build:ios:consultant:staging": "eas build --profile consultant-staging --platform ios --local",
    "build:ios:consultant:prod": "eas build --profile consultant-production --platform ios --local",
    "build:android:customer:staging": "eas build --profile customer-staging --platform android --local",
    "build:android:customer:prod": "eas build --profile customer-production --platform android --local",
    "build:android:consultant:staging": "eas build --profile consultant-staging --platform android --local",
    "build:android:consultant:prod": "eas build --profile consultant-production --platform android --local",
    "typecheck": "tsc --noEmit",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "axios": "^1.13.2",
    "expo": "~54.0.30",
    "expo-constants": "~18.0.12",
    "expo-font": "^14.0.10",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "^31.0.13",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-error-boundary": "^6.0.2",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "sentry-expo": "^7.2.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "axios-mock-adapter": "^2.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.9",
    "dotenv": "^17.2.3",
    "typescript": "~5.9.2"
  }
}
